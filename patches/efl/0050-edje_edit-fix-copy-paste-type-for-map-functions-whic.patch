+++ b/src/lib/edje/edje_edit.c
@@ -7736,7 +7736,7 @@ edje_edit_state_map_perspective_zplane_set(Evas_Object *obj, const char *part, c
    return EINA_TRUE;
 }
 
-EAPI Eina_Bool
+EAPI int

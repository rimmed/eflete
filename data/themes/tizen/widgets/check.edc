         signal: "elm,state,check,on";
         source: "elm";
         filter: "dummy.selected" "default";
         action: STATE_SET "default" 0.00;
         target: "check";
      }
      program { name: "selected";
         signal: "selected";
         source: "eflete";
         action: STATE_SET "selected" 0.00;
         target: "dummy.selected";
      }
      program { name: "unselected";
         signal: "unselected";
         source: "eflete";
         action: STATE_SET "default" 0.00;
         target: "dummy.selected";
      }
   }
}


group { name: "elm/check/base/toggle";
   images {
      image: "General/toggle_off.png" COMP;
      image: "General/toggle_on.png" COMP;
   }

   parts {
      part { name: "elm.swallow.content";
         type: SWALLOW;
         scale: 1;
         clip_to: "clipper";
         description { state: "default" 0.0;
            visible: 0;
            align: 0.0 0.5;
            fixed: 1 0;
            rel2 {
               relative: 0.0 1.0;
            }
         }
         description { state: "visible" 0.0;
            align: 0.0 0.5;
            min: 19 19;
            max: 0 0;
            fixed: 1 0;
            rel2 {
               relative: 0.0 1.0;
            }
         }
      }
      part { name: "padding.icon";
         type: SPACER;
         description { state: "default" 0.0;
            align: 0.0 0.5;
            max: 0 -1;
            rel1 {
               relative: 1.0 0.0;
               to: "elm.swallow.content";
            }
            rel2 {
               to_y: "elm.swallow.content";
            }
         }
         description { state: "visible" 0.0;
            align: 0.0 0.5;
            min: 2 0;
            max: 2 -1;
            rel1 {
               relative: 1.0 0.0;
               to: "elm.swallow.content";
            }
            rel2 {
               to_y: "elm.swallow.content";
            }
         }
      }
      part { name: "elm.text";
         type: TEXT;
         scale: 1;
         clip_to: "clipper";
         description { state: "default" 0.0;
            visible: 0;
            align: 0.0 0.5;
            max: 0 -1;
            color_class: "main";
            rel1 {
               to_x: "padding.icon";
               relative: 1.0 0.0;
            }
            text {
               font: FN;
               size: 12;
               max: 1 1;
            }
         }
         description { state: "visible" 0.0;
            align: 0.0 0.5;
            color_class: "main";
            rel1 {
               relative: 1.0 0.0;
               to_x: "padding.icon";
            }
            text {
               font: FN;
               size: 12;
               max: 1 1;
            }
         }
      }
      part { name: "padding.text";
         type: SPACER;
         description { state: "default" 0.0;
            align: 0.0 0.5;
            max: 0 -1;
            rel1 {
               relative: 1.0 0.0;
               to: "elm.text";
            }
            rel2 {
               to_y: "elm.text";
            }
         }
         description { state: "visible" 0.0;
            align: 0.0 0.5;
            min: 2 0;
            max: 0 -1;
            rel1 {
               relative: 1.0 0.0;
               to: "elm.text";
            }
            rel2 {
               to_y: "elm.text";
            }
         }
      }
      part { name: "base";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            min: 76 24;
            max: 76 24;
            rel1 {
               relative: 1.0 0.0;
               to_x: "padding.text";
            }
         }
      }
      part { name: "button";
         type: IMAGE;
         scale: 1;
         clip_to: "clipper";
         description { state: "default" 0.0;
            rel1.to: "base";
            rel2.to: "base";
            image {
               normal: "General/toggle_off.png";
            }
         }
         description { state: "on" 0.0;
            inherit: "default" 0.0;
            image.normal: "General/toggle_on.png";
         }
      }
      part { name: "event.button";
         type: RECT;
         repeat_events: 1;
         description { state: "default" 0.0;
            color: 0 0 0 0;
            rel1 {
               to_x: "elm.swallow.content";
               to_y: "base";
            }
            rel2 {
               to: "base";
            }
         }
      }
      part { name: "clipper";
         type: RECT;
         description { state: "default" 0.0;
         }
      }
   }
   programs {
      program { name: "icon_show";
         signal: "elm,state,icon,visible";
         source: "elm";
         action: STATE_SET "visible" 0.00;
         target: "elm.swallow.content";
         target: "padding.icon";
      }
      program { name: "icon_hide";
         signal: "elm,state,icon,hidden";
         source: "elm";
         action: STATE_SET "default" 0.00;
         target: "elm.swallow.content";
         target: "padding.icon";
      }
      program { name: "text_show";
         signal: "elm,state,text,visible";
         source: "elm";
         action: STATE_SET "visible" 0.00;
         target: "elm.text";
         target: "padding.text";
      }
      program { name: "text_hide";
         signal: "elm,state,text,hidden";
         source: "elm";
         action: STATE_SET "default" 0.00;
         target: "elm.text";
         target: "padding.text";
      }
      program { name: "off";
         signal: "elm,state,check,off";
         source: "elm";
         action: STATE_SET "default" 0.00;
         target: "button";
      }
      program { name: "on";
         signal: "elm,state,check,on";
         source: "elm";
         action: STATE_SET "on" 0.00;
         target: "button";
      }
      program { name: "clicked_emit";
         signal: "mouse,clicked,1";
         source: "event.button";
         action: SIGNAL_EMIT "elm,action,check,toggle" "elm";
      }
   }
}

group { name: "elm/check/base/default";
   images {
      image: "Tizen general/check_nor.png" COMP;
      image: "Tizen general/check_nor_disable.png" COMP;
      image: "Tizen general/check_nor_focused.png" COMP;
      image: "Tizen general/check_nor_hover.png" COMP;
      image: "Tizen general/check_sel.png" COMP;
      image: "Tizen general/check_sel_disable.png" COMP;
      image: "Tizen general/check_sel_focused.png" COMP;
      image: "Tizen general/check_sel_hover.png" COMP;
   }
   parts {
      part { name: "dummy";
         type: SPACER;
         description { state: "default";
         }
         description { state: "visible";
         }
      }
      part { name: "bg";
         type: IMAGE;
         mouse_events: 0;
         scale: 1;
         description { state: "default" 0.0;
            align: 0.0 0.5;
            min: 14 14;
            max: 14 14;
            aspect: 1.0 1.0;
            image.normal: "Tizen general/check_nor.png";
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.00;
            image.normal: "Tizen general/check_nor_disable.png";
         }
         description { state: "hovered" 0.0;
            inherit: "default" 0.00;
            image.normal: "Tizen general/check_nor_hover.png";
         }
         description { state: "focused" 0.0;
            inherit: "default" 0.00;
            image.normal: "Tizen general/check_nor_focused.png";
         }
       }
      part { name: "check";
         type: IMAGE;
         mouse_events: 0;
         scale: 1;
         clip_to: "clipper";
         description { state: "default" 0.0;
            visible: 0;
            image.normal: "Tizen general/check_sel.png";
            rel1.to: "bg";
            rel2.to: "bg";
         }
         description { state: "visible" 0.0;
            inherit: "default" 0.00;
            visible: 1;
         }
         description { state: "disabled" 0.0;
            inherit: "visible" 0.00;
            image.normal: "Tizen general/check_sel_disable.png";
         }
         description { state: "hovered" 0.0;
            inherit: "visible" 0.00;
            image.normal: "Tizen general/check_sel_hover.png";
         }
         description { state: "focused" 0.0;
            inherit: "visible" 0.00;
            image.normal: "Tizen general/check_sel_focused.png";
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         scale: 1;
         clip_to: "clipper";
         description { state: "default" 0.0;
            visible: 0;
            align: 0.0 0.5;
            fixed: 1 0;
            max: 0 -1;
            rel1 {
               relative: 1.0 0.0;
               to_x: "bg";
            }
            rel2 {
               offset: 1 -2;
               to_x: "bg";
            }
         }
         description { state: "visible" 0.0;
            inherit: "default" 0.00;
            visible: 1;
            rel1.offset: 6 0;
         }
      }
      part { name: "elm.text";
         type: TEXT;
         scale: 1;
         clip_to: "clipper";
         description { state: "default" 0.0;
            visible: 0;
            fixed: 0 1;
            text {
               font: FN;
               size: 10;
               min: 1 1;
               align: 0.0 0.5;
               ellipsis: -1.0;
            }
            rel1 {
               relative: 1.0 0.0;
               to_x: "elm.swallow.content";
               to_y: "bg";
            }
            rel2.to_y: "bg";
         }
         description { state: "visible" 0.0;
            inherit: "default" 0.00;
            visible: 1;
            color_class: "genlist_text_tree";
            rel1.offset: 6 0;
         }
      }
      part { name: "clipper";
         type: RECT;
         repeat_events: 1;
         description { state: "default" 0.0;
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.00;
         }
      }
      part { name: "events";
         type: RECT;
         ignore_flags: "ON_HOLD";
         description { state: "default" 0.0;
            color: 0 0 0 0;
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.00;
            visible: 0;
         }
      }
   }
   programs {
      program { name: "click";
         signal: "mouse,clicked,1";
         source: "events";
         action: SIGNAL_EMIT "elm,action,check,toggle" "";
      }
      program { name: "check_on";
         signal: "elm,state,check,on";
         source: "elm";
         action: STATE_SET "visible" 0.00;
         target: "check";
         target: "dummy";
      }
      program { name: "check_off";
         signal: "elm,state,check,off";
         source: "elm";
         action: STATE_SET "default" 0.00;
         target: "check";
         target: "dummy";
      }
      program { name: "text_show";
         signal: "elm,state,text,visible";
         source: "elm";
         action: STATE_SET "visible" 0.00;
         target: "elm.text";
      }
      program { name: "text_hide";
         signal: "elm,state,text,hidden";
         source: "elm";
         action: STATE_SET "default" 0.00;
         target: "elm.text";
      }
      program { name: "icon_show";
         signal: "elm,state,icon,visible";
         source: "elm";
         action: STATE_SET "visible" 0.00;
         target: "elm.swallow.content";
      }
      program { name: "icon_hide";
         signal: "elm,state,icon,hidden";
         source: "elm";
         action: STATE_SET "default" 0.00;
         target: "elm.swallow.content";
      }
     program { name: "mouse_in_bg";
         signal: "mouse,in";
         source: "events";
         filter: "dummy" "default";
         action: STATE_SET "hovered" 0.00;
         target: "bg";
      }
      program { name: "mouse_in_check";
         signal: "mouse,in";
         source: "events";
         filter: "dummy" "visible";
         action: STATE_SET "hovered" 0.00;
         target: "check";
      }
      program { name: "mouse_out_bg";
         signal: "mouse,out";
         source: "events";
         filter: "dummy" "default";
         action: STATE_SET "default" 0.00;
         target: "bg";
      }
      program { name: "mouse_out_check";
         signal: "mouse,out";
         source: "events";
         filter: "dummy" "visible";
         action: STATE_SET "visible" 0.00;
         target: "check";
      }
      program { name: "disable_check";
         signal: "elm,state,disabled";
         source: "elm";
         filter: "dummy" "visible";
         action: STATE_SET "disabled" 0.00;
         target: "check";
         target: "events";
         target: "clipper";
      }
      program { name: "enable_check";
         signal: "elm,state,enabled";
         source: "elm";
         filter: "dummy" "visible";
         action: STATE_SET "visible" 0.00;
         target: "check";
         target: "events";
         target: "clipper";
      }
      program { name: "focused_bg";
         signal: "elm,action,focus";
         source: "elm";
         filter: "dummy" "default";
         action: STATE_SET "focused" 0.00;
         target: "bg";
      }
      program { name: "unfocused_bg";
         signal: "elm,action,unfocus";
         source: "elm";
         filter: "dummy" "default";
         action: STATE_SET "default" 0.00;
         target: "bg";
      }
      program { name: "focused_check";
         signal: "elm,action,focus";
         source: "elm";
         filter: "dummy" "visible";
         action: STATE_SET "focused" 0.00;
         target: "check";
      }
      program { name: "unfocused_check";
         signal: "elm,action,unfocus";
         source: "elm";
         filter: "dummy" "visible";
         action: STATE_SET "visible" 0.00;
         target: "check";
      }
      program { name: "disable_bg";
         signal: "elm,state,disabled";
         source: "elm";
         filter: "dummy" "default";
         action: STATE_SET "disabled" 0.00;
         target: "bg";
         target: "events";
         target: "clipper";
      }
      program { name: "enable_bg";
         signal: "elm,state,enabled";
         source: "elm";
         filter: "dummy" "default";
         action: STATE_SET "default" 0.00;
         target: "bg";
         target: "events";
         target: "clipper";
      }
   }
}


group { name: "elm/panes/vertical/default";
   images {
      image: "panes_vertical.png" COMP;
      image: "window_scale_controller_v.png" COMP;
   }
   parts {
      part { name: "whole";
         type: SPACER;
         description { state: "default" 0.0;
         }
      }
      part { name: "right_constraint";
         type: SPACER;
         dragable {
            x: 1 1 1;
            y: 0 0 0;
            confine: "whole";
         }
         description { state: "default" 0.0;
            max: 0 -1;
         }
      }
      part { name: "left_constraint";
         type: SPACER;
         dragable {
            x: 1 1 1;
            y: 0 0 0;
            confine: "whole";
         }
         description { state: "default" 0.0;
            max: 0 -1;
         }
      }
      part { name: "bar_area";
         type: SPACER;
         description { state: "default" 0.0;
            rel1.to_x: "left_constraint";
            rel2.to_x: "right_constraint";
         }
      }
      part { name: "elm.bar";
         type: SPACER;
         dragable {
            x: 1 1 0;
            y: 0 0 0;
            confine: "bar_area";
         }
         description { state: "default" 0.0;
            min: 2 0;
            max: 2 -1;
         }
      }
      part { name: "elm.swallow.event";
         type: SWALLOW;
         description { state: "default" 0.0;
            visible: 0;
            fixed: 1 1;
            min: 6 0;
            rel1.to: "bar";
            rel2.to: "bar";
         }
      }
      part { name: "bar";
         type: RECT;
         description { state: "default" 0.0;
            color: 177 177 177 255;
            rel1.to_x: "elm.bar";
            rel2.to_x: "elm.bar";
         }
         description { state: "disabled" 0.0;
            visible: 0;
            rel1.to_y: "elm.swallow.event";
            rel2.to_y: "elm.swallow.event";
         }
      }
      part { name: "hightlight";
         type: RECT;
         description { state: "default" 0.0;
            min: 2 0;
            max: 2 -1;
            color: 177 177 177 255;
            rel1.to: "bar";
            rel2.to: "bar";
         }
         description { state: "focused" 0.0;
            inherit: "default" 0.00;
            color: 191 193 196 255;
         }
         description { state: "clicked" 0.0;
            inherit: "focused" 0.00;
            color: 185 187 190 255;
         }
      }
      part { name: "events";
         type: RECT;
         dragable {
            events: "elm.bar";
         }
         description { state: "default" 0.0;
            color: 0 0 0 0;
            rel1.to: "bar";
            rel2.to: "bar";
         }
      }
      part { name: "clipper_left";
         type: RECT;
         description { state: "default" 0.0;
            rel2 {
               relative: 0.0 1.0;
               to_x: "bar";
            }
         }
      }
      part { name: "clipper_right";
         type: RECT;
         description { state: "default" 0.0;
            rel1 {
               relative: 1.0 0.0;
               to_x: "bar";
            }
         }
      }
      part { name: "elm.swallow.left";
         type: SWALLOW;
         clip_to: "clipper_left";
         description { state: "default" 0.0;
            fixed: 1 1;
            rel2 {
               relative: 0.0 1.0;
               to_x: "bar";
            }
         }
      }
      part { name: "elm.swallow.right";
         type: SWALLOW;
         clip_to: "clipper_right";
         description { state: "default" 0.0;
            fixed: 1 1;
            rel1 {
               relative: 1.0 0.0;
               to_x: "bar";
            }
         }
      }
   }
   programs {
      program { name: "button_down_double";
         signal: "mouse,down,1,double";
         source: "events";
         action: SIGNAL_EMIT "elm,action,click,double" "";
      }
      program { name: "button_click";
         signal: "mouse,down,1";
         source: "events";
         action: SIGNAL_EMIT "elm,action,press" "";
         after: "click_anim";
      }
      program { name: "button_unclick";
         signal: "mouse,up,1";
         source: "events";
         action: SIGNAL_EMIT "elm,action,unpress" "";
         after: "unclick_anim";
      }
      program { name: "highlight_on";
         signal: "mouse,in";
         source: "events";
         action: STATE_SET "focused" 0.00;
         transition: ACCELERATE 0.50000;
         target: "hightlight";
      }
      program { name: "highlight_off";
         signal: "mouse,out";
         source: "events";
         action: STATE_SET "default" 0.00;
         transition: ACCELERATE 0.50000;
         target: "hightlight";
      }
      program { name: "disable";
         signal: "elm,state,disabled";
         source: "elm";
         action: STATE_SET "disabled" 0.00;
         target: "bar";
      }
      program { name: "enable";
         signal: "elm,state,enabled";
         source: "elm";
         action: STATE_SET "default" 0.00;
         target: "bar";
      }
      program { name: "fixed";
         signal: "elm.panes.fixed";
         source: "elm";
         action: STATE_SET "disabled" 0.00;
         target: "bar";
      }
      program { name: "unfixed";
         signal: "elm.panes.unfixed";
         source: "elm";
         action: STATE_SET "default" 0.00;
         target: "bar";
      }
      program { name: "click_anim";
         action: STATE_SET "clicked" 0.00;
         transition: ACCELERATE 0.25000;
         target: "hightlight";
      }
      program { name: "unclick_anim";
         action: STATE_SET "default" 0.00;
         transition: ACCELERATE 0.25000;
         target: "hightlight";
      }
   }
}


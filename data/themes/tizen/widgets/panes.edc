      part { name: "elm.bar";
         type: SPACER;
         dragable {
            x: 0 0 0;
            y: 1 1 0;
            confine: "bar_area";
         }
         description { state: "default" 0.0;
            align: 0.5 0.0;
            fixed: 1 1;
            min: 0 2;
            max: -1 2;
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.00;
            min: 0 0;
            max: -1 0;
         }
      }
      part { name: "bar";
         type: RECT;
         description { state: "default" 0.0;
            fixed: 1 0;
            min: 0 2;
            max: -1 2;
            color: 177 177 177 255;
            rel1.to_y: "elm.swallow.event";
            rel2.to_y: "elm.swallow.event";
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.00;
            visible: 0;
            min: 6 6;
            max: -1 6;
         }
      }
      part { name: "hightlight";
         type: RECT;
         description { state: "default" 0.0;
            min: 0 2;
            max: -1 2;
            color: 177 177 177 255;
            rel1.to: "bar";
            rel2.to: "bar";
         }
         description { state: "focused" 0.0;
            inherit: "default" 0.00;
            color: 191 193 196 255;
         }
         description { state: "clicked" 0.0;
            inherit: "focused" 0.00;
            color: 185 187 190 255;
         }
      }
      part { name: "clipper";
         type: RECT;
         dragable {
            events: "elm.bar";
         }
         description { state: "default" 0.0;
            fixed: 1 1;
            color: 0 0 0 0;
            rel1.to: "bar";
            rel2.to: "bar";
         }
      }
      part { name: "whole_left";
         type: RECT;
         description { state: "default" 0.0;
            rel2 {
               relative: 1.0 0.0;
               to_y: "elm.bar";
            }
         }
      }
      part { name: "whole_right";
         type: RECT;
         description { state: "default" 0.0;
            rel1 {
               relative: 0.0 1.0;
               to_y: "elm.bar";

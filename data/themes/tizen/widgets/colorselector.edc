group { name: "elm/colorselector/item/default";
   parts {
      part { name: "base";
         type: RECT;
         description { state: "default" 0.0;
            min: 27 28;
            max: 27 28;
            visible: 0;
         }
      }
      part { name: "bg";
         type: RECT;
         description { state: "default" 0.0;
            min: 18 19;
            max: 18 19;
         }
      }
      part { name: "color_obj";
         type: SWALLOW;
         clip_to: "clipper";
         description { state: "default" 0.0;
            min: 16 17;
            max: 16 17;
            rel1.offset: -1 -1;
         }
      }
     part { name: "glow";
         type: IMAGE;
         mouse_events: 0;
         repeat_events: 1;
         description { state: "default" 0.0;
            min: 18 19;
            max: 18 19;
            visible: 0;
            rel1.to: "bg";
            rel2.to: "bg";
            rel1.offset: -1 -1;
            rel2.offset: 0 0;
         }
         description { state: "focused" 0.0;
            inherit: "default" 0.0;
            color_class: "over";
         }
      }
      part { name: "clipper";
         type: RECT;
         description { state: "default" 0.0;
            rel1.to: "color_obj";
            rel2.to: "color_obj";
         }
         description { state: "disabled" 0.0;
            color_class: "main_disabled";
            rel1.to: "color_obj";
            rel2.to: "color_obj";
         }
      }
      part { name: "disabler";
         type: RECT;
         description { state: "default" 0.0;
            visible: 0;
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.0;
            visible: 1;
         }
      }
   }
   programs {
      program { name: "program_0x1be4380";
         signal: "elm,state,selected";
         source: "elm";
         action: STATE_SET "focused" 0.00;
         transition: DECELERATE 0.10000;
         target: "glow";
      }
      program { name: "program_0x1be4870";
         signal: "elm,state,unselected";
         source: "elm";
         action: STATE_SET "default" 0.00;
         transition: DECELERATE 0.50000;
         target: "glow";
      }
      program { name: "program_0x1be4d60";
         signal: "elm,anim,activate";
         source: "elm";
         action: STATE_SET "focused" 0.00;
         transition: DECELERATE 0.10000;
         target: "glow";
         after: "active2";
      }
      program { name: "active2";
         action: STATE_SET "default" 0.00;
         transition: DECELERATE 0.80000;
         target: "glow";
      }
      program {
         signal: "elm,state,disabled"; source: "elm";
         action: STATE_SET "disabled" 0.0;
         target: "clipper";
         target: "disabler";
      }
      program {
         signal: "elm,state,enabled"; source: "elm";
         action: STATE_SET "default" 0.0;
         target: "clipper";
         target: "disabler";
      }
   }
}


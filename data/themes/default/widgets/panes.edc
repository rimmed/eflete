      part { name: "whole";
         type: SPACER;
         description { state: "default" 0.0;
         }
      }
      part { name: "right_constraint";
         type: SPACER;
         dragable {
            x: 0 0 0;
            y: 1 1 1;
            confine: "whole";
         }
         description { state: "default" 0.0;
            max: -1 0;
         }
      }
      part { name: "left_constraint";
         type: SPACER;
         dragable {
            x: 0 0 0;
            y: 1 1 1;
            confine: "whole";
         }
         description { state: "default" 0.0;
            max: -1 0;
         }
      }
      part { name: "bar_area";
         type: SPACER;

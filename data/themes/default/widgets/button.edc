group { name: "elm/button/base/recent";
   images {
      image: "icon_file.png" COMP;
      image: "control-focus.png" COMP;
   }
   parts {
      part { name: "base";
         type: SPACER;
         description { state: "default" 0.0;
            min: 0 32;
         }
      }
      part { name: "icon";
         type: IMAGE;
         description { state: "default" 0.0;
            max: 14 14;
            align: 0.0 0.5;
            color_class: "main_alternative";
            rel1 {
               offset: 8 0;
            }
            image {
               normal: "icon_file.png";
            }
         }
         description { state: "clicked" 0.0;
            inherit: "default" 0.0;
            color_class: "select";
         }
      }
      part { name: "elm.text";
         type: TEXT;
         scale: 1;
         description { state: "default" 0.0;
            color_class: "main_alternative";
            rel1 {
               to_x: "icon";
               relative: 1.0 0.0;
               offset: 6 2;
            }
            rel2 {
               offset: -7 -1;
            }
            text {
               font: "PT";
               size: 12;
               align: 0.0 0.0;
            }
         }
         description { state: "clicked" 0.0;
            inherit: "default" 0.0;
            color_class: "select";
         }
      }
      part { name: "elm.subtext";
         type: TEXT;
         scale: 1;
         description { state: "default" 0.0;
            color_class: "main_disabled";
            rel1 {
               to_x: "icon";
               relative: 1.0 0.0;
               offset: 6 0;
            }
            rel2 {
               offset: -7 -5;
            }
            text {
               font: "PT";
               size: 10;
               align: 0.0 1.0;
            }
         }
         description { state: "clicked" 0.0;
            inherit: "default" 0.0;
            color_class: "select";
         }
      }
      part { name: "focus";
         type: IMAGE;
         repeat_events: 1;
         description { state: "default" 0.0;
            visible: 0;
            color_class: "select";
            rel1 {
               to: "base";
            }
            rel2 {
               to: "base";
            }
            image {
               normal: "control-focus.png";
               border: 8 8 8 8;
               middle: DEFAULT;
            }
         }
         description { state: "clicked" 0.0;
            inherit: "default" 0.0;
            visible: 1;
            color_class: "select";
         }
      }
      part { name: "event";
         type: RECT;
         description { state: "default" 0.0;
            color: 0 0 0 0;
         }
      }
   }
   programs {
      program { name: "button_click";
         signal: "mouse,down,1";
         source: "event";
         action: SIGNAL_EMIT "elm,action,press" "elm";
         after: "button_click_anim";
      }
      program { name: "button_unclick";
         signal: "mouse,up,1";
         source: "event";
         action: SIGNAL_EMIT "elm,action,unpress" "elm";
         after: "button_unclick_anim";
      }
      program { name: "button_click_anim";
         action: STATE_SET "clicked" 0.00;
         target: "icon";
         target: "elm.text";
         target: "elm.subtext";
         target: "focus";
      }
      program { name: "button_unclick_anim";
         action: STATE_SET "default" 0.00;
         target: "icon";
         target: "elm.text";
         target: "elm.subtext";
         target: "focus";
      }
      program { name: "button_pressed_anim";
         signal: "elm,anim,activate";
         source: "elm";
         action: STATE_SET "clicked" 0.00;
         target: "icon";
         target: "elm.text";
         target: "elm.subtext";
         target: "focus";
         after: "button_unpressed_anim";
      }
      program { name: "button_unclick2";
         signal: "mouse,clicked,1";
         source: "event";
         action: SIGNAL_EMIT "elm,action,click" "";
      }
      program { name: "button_unpressed_anim";
         action: STATE_SET "default" 0.00;
         target: "icon";
         target: "elm.text";
         target: "elm.subtext";
         target: "focus";
      }
      program { name: "focus,in";
         signal: "mouse,in";
         source: "event";
         action: STATE_SET "clicked" 0.00;
         target: "focus";
      }
      program { name: "focus,out";
         signal: "mouse,out";
         source: "event";
         action: STATE_SET "default" 0.00;
         target: "focus";
      }
   }
}

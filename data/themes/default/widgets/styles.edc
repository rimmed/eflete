      tag:  "br" "\n";
      tag:  "ps" "ps";
      tag:  "whitecolor" "+ color=#ffffff";
      tag:  "tab" "\t";
   }
   style { name: "popup_title_style";
base: "font="FN_B" font_size=13 color=#d6d6d6 wrap=none text_class=label align=center ellipsis=1.0";
      tag:  "br" "\n";
      tag:  "tab" "\t";
   }

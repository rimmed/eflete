group { name: "elm/layout/groupview/default";
   parts {
      images {
         image: "bg_proxy.png" COMP;
         image: "bg_swallow.png" COMP;
         image: "bg_spacer.png" COMP;
         image: "bg_group.png" COMP;
         image: "bg_table.png" COMP;
         image: "bg_box.png" COMP;
         image: "bg_part_item.png" COMP;
         image: "bg_object_area.png" COMP;
      }
      part { name: "bg";
         type: IMAGE;
         description { state: "default" 0.0;
            visible: 0;
            image {
               normal: "bg_proxy.png";
            }
            fill.type: TILE;
         }
         description { state: "text" 0.0;
            inherit: "default" 0.0;
         }
#define BG(NAME, IMG) \
   description { state: #NAME 0.0; \
      inherit: "default" 0.0; \
      visible: 1; \
      image.normal: #IMG; \
   }
         BG(proxy,         bg_proxy.png)
         BG(swallow,       bg_swallow.png)
         BG(spacer,        bg_spacer.png)
         BG(group,         bg_group.png)
         BG(table,         bg_table.png)
         BG(box,           bg_box.png)
         BG(object_area,   bg_object_area.png)
#undef BG
         description { state: "part_item" 0.0;
            inherit: "default" 0.0;
         }
         description { state: "part_item_hilight" 0.0;
            inherit: "default" 0.0;
            image.normal: "bg_part_item.png";
         }
         description { state: "part_item_hilight" 1.0;
            inherit: "default" 0.0;
            visible: 1;
            image.normal: "bg_part_item.png";
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         description { state: "default" 0.0;
            visible: 0;
         }
      }
      part { name: "border";
         type: IMAGE;
         description { state: "default" 0.0;
            color:  0 0 0 0;
            image {
               normal: "border_1.png";
               border: 1 1 1 1;
               middle: NONE;
            }
         }
#define BORDER(NAME, COLOR) \
   description { state: #NAME 0.0; \
      inherit: "default" 0.0; \
      color: COLOR; \
   }
         BORDER(text,         122 122 122 255)
         BORDER(proxy,        255 112 49 255)
         BORDER(swallow,      120 103 140 255)
         BORDER(spacer,       101 117 133 255)
         BORDER(group,        255 109 109 255)
         BORDER(table,        138 125 109 255)
         BORDER(box,          124 129 102 255)
         BORDER(part_item,    49 140 141 255)
         BORDER(object_area,  49 140 141 255)
#undef BORDER
      }
   }
   programs {
#define PROGRAM(SIGNAL) \
   program { name: #SIGNAL; \
      source: "eflete"; \
      signal: "border,"#SIGNAL; \
      action: STATE_SET #SIGNAL; \
      target: "border"; \
      target: "bg"; \
   }
      PROGRAM(default)
      PROGRAM(text)
      PROGRAM(proxy)
      PROGRAM(swallow)
      PROGRAM(spacer)
      PROGRAM(group)
      PROGRAM(table)
      PROGRAM(box)
      PROGRAM(part_item)
      PROGRAM(object_area)
#undef PROGRAM
   }
   program { name: "part_item_hilight_off";
      source: "eflete";
      filter: "border" "part_item" 0.0;
      signal: "border,part_item,hilight,off";
      action: STATE_SET part_item_hilight 0.0;
      target: "bg";
   }
   program { name: "part_item_hilight_on";
      source: "eflete";
      filter: "border" "part_item" 0.0;
      signal: "border,part_item,hilight,on";
      action: STATE_SET part_item_hilight 1.0;
      target: "bg";
   }
}


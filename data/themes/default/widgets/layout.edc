group { name: "elm/layout/window/main";
   images {
      image: "status-top_border-bg.png" COMP;
   }
   parts {
      part { name: "eflete.swallow.panes";
         type: SWALLOW;
         scale: 1;
         mouse_events: 1;
         description { state:   "default" 0.0;
            rel2 {
               relative: 1.0 0.0;
               to: "statusbar";
            }
          }
      }
      part { name: "statusbar";
         type: SPACER;
         scale: 1;
         description { state: "default" 0.0;
            max: -1 20;
            align: 0.5 1.0;
         }
      }
      part { name: "statusbar_border";
         type: IMAGE;
         description { state: "default" 0.0;
            align: 0.5 0.0;
            max: -1 2;
            rel1 {
               to: "statusbar";
            }
            image {
               normal: "status-top_border-bg.png";
            }
         }
      }
      part { name: "eflete.project.time";
         type: TEXT;
         description {
            max: 500 -1;
            align: 0.0 0.5;
            rel1 {
               to: "statusbar";
               offset: 3 0;
            }
            rel2 {
               to: "statusbar";
            }
            text {
               size: 11;
               font: FN;
               align: 0.0 0.5;
            }
         }
      }
      part { name: "eflete.project.part";
         type: TEXT;
         description {
            align: 0.0 0.5;
            rel1 {
               to_x: "eflete.project.time";
               to_y: "statusbar";
               relative: 1.0 0.0;
            }
            rel2 {
               to: "statusbar";
            }
            text {
               size: 11;
               font: FN;
               align: 0.0 0.5;
            }
         }
      }
   }
}

group { name: "elm/layout/popup/hint";
   parts {
      part { name: "event";
         type: RECT;
         description { state: "default" 0.0;
            rel1.offset: -9999 -9999;
            rel2.offset: 9999 9999;
            color: 0 0 0 0;
         }
      }
      part { name: "bg";
         type: IMAGE;
         description { state: "default" 0.0;
            rel1 {
               offset: -6 -6;
            }
            rel2 {
               offset: 6 6;
            }
            image {
               normal: "hint-background.png";
               border: 5 5 3 3;
               middle: DEFAULT;
            }
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         description { state: "default" 0.0;
            align: 0.0 0.0;
            rel1 {
               offset: 6 6;
               to: "bg";
            }
            rel2 {
               relative: 1.0 0.0;
               offset: -7 -7;
               to_x: "bg";
               to_y: "elm.swallow.ok";
            }
         }
      }
      part { name: "elm.swallow.ok";
         type: SWALLOW;
         description { state: "default" 0.0;
            align: 1.0 1.0;
            max: 32 16;
            rel2 {
               offset: -7 -7;
               to: "bg";
            }
         }
      }
   }
   programs {
      program { name: "dismiss";
         signal: "mouse,up,*";
         source: "event";
         action: SIGNAL_EMIT "hint,dismiss" "eflete";
      }
   }
}


group { name: "elm/layout/popup/hint_title";
   images {
      image: "hint-background.png" COMP;
   }
   parts {
      part { name: "event";
         type: RECT;
         description { state: "default" 0.0;
            rel1.offset: -9999 -9999;
            rel2.offset: 9999 9999;
            color: 0 0 0 0;
         }
      }
      part { name: "bg";
         type: IMAGE;
         description { state: "default" 0.0;
            rel1 {
               to_y: "elm.text";
               to_x: "elm.swallow.content";
               offset: -7 -7;
            }
            rel2 {
               to: "elm.swallow.content";
               offset: 6 6;
            }
            image {
               normal: "hint-background.png";
               border: 5 5 3 3;
            }
         }
      }
      part { name: "elm.text";
         type: TEXT;
         description { state: "default" 0.0;
            align: 0.5 1.0;
            fixed: 1 1;
            color_class: "main";
            rel1 {
               to_x: "elm.swallow.content";
               offset: 0 6;
            }
            rel2 {
               to: "elm.swallow.content";
               relative: 1.0 0.0;
               offset: 0 -7;
            }
            text {
               font: FN;
               size: 16;
               min: 1 1;
               elipsis: -1;
            }
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         description { state: "default" 0.0;
            align: 0.0 0.0;
         }
      }
   }
   programs {
      program { name: "dismiss";
         signal: "mouse,up,1";
         source: "event";
         action: SIGNAL_EMIT "hint,dismiss" "eflete";
      }
   }
}




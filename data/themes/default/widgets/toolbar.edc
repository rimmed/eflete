               to_x: "clipper_arrow_left";
               to_y: "elm.swallow.content";
            }
            image {
               normal: "icon_arrow_left.png";
            }
         }
      }
      part { name: "clipper_arrow_right";
         type: RECT;
         description { state: "default" 0.0;
            min: 18 0;
            max: 18 -1;
            align: 1.0 0.5;

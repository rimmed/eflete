               relative: 1.0 0.0;
            }
            rel2 {
               relative: 0.0 1.0;
               to_x: "clipper_arrow_right";
               to_y: "separator";
            }
         }
      }
   }
   programs {
      program { name: "arrow_left_focused";
         signal: "mouse,in";
         source: "arrow_left";
         action: STATE_SET "focused" 0.0;
         target: "arrow_left";
      }
      program { name: "arrow_left_unfocused";
         signal: "mouse,out";
         source: "arrow_left";
         action: STATE_SET "default" 0.0;
         target: "arrow_left";
      }
      program { name: "arrow_left_clicked";
         signal: "mouse,down,1";
         source: "arrow_left";
         action: STATE_SET "clicked" 0.0;
         target: "arrow_left";
      }
      program { name: "arrow_left_click_emit";
         signal: "mouse,down,1";
         source: "arrow_left";
         action: SIGNAL_EMIT "elm,action,left" "elm";
         after: "arrow_left_repeat";
      }
      program { name: "arrow_left_repeat";
         action: SIGNAL_EMIT "elm,action,left" "elm";

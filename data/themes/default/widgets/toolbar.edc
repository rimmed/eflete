            rel2.offset: -7 -1;
            image.normal: "icon_close.png";
            color_class: "main";
         }
         description { state: "focused" 0.0;
            inherit: "default" 0.0;
            color_class: "select_light";
         }
         description { state: "clicked" 0.0;
            inherit: "default" 0.0;
            color_class: "select";
         }
      }
      part { name: "elm.text";
         type: TEXT;
         clip_to: "clipper";
         description { state: "default" 0.0;
            min: 0 21;
            rel1 {
               relative: 1.0 0.0;
               to_x: "elm.swallow.icon";
            }
            text {
               font: "PT";
               size: 12;
               max: 1 0;
            }
         }
         description { state: "visible" 0.0;
            min: 120 21;
            rel1 {
               relative: 1.0 0.0;
               offset: 6 0;
               to_x: "elm.swallow.icon";
            }
            rel2 {
               to_x: "icon_close";
               relative: 0.0 1.0;
               offset: -5 -1;
            }

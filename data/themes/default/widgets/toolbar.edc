         type: RECT;
         repeat_events: 1;
         description { state: "default" 0.0;
            color: 255 255 255 0;
         }
         description { state: "disabled" 0.0;
            visible: 0;
            color: 255 255 255 0;
         }
      }
   }
   programs {
      program { name: "mouse_down";
         signal: "mouse,down,1";
         source: "event";
         action: STATE_SET "selected" 0.00;
         target: "bg";
      }
      program { name: "mouse_up";
         signal: "mouse,up,1";
         source: "event";
         action: SIGNAL_EMIT "elm,action,click" "elm";
      }
      program { name: "text_hide";
         signal: "elm,state,text,hidden";
         source: "elm";
         script {

         new st[31];
         new Float:vl;
         get_state(PART:"elm.swallow.icon", st, 30, vl);
         if (!strcmp(st, "visible")) {
            set_state(PART:"elm.swallow.icon", "icononly", 0.0);
         }
         set_state(PART:"elm.text", "default", 0.0);
                  }
      }
      program { name: "selected";
         signal: "elm,state,selected";
         source: "elm";
         action: STATE_SET "selected" 0.0;
         target: "bg";
      }
      program { name: "unselected";
         signal: "elm,state,unselected";
         source: "elm";
         action: STATE_SET "default" 0.0;
         target: "bg";
      }
      program { name: "text_show";
         signal: "elm,state,text,visible";
         source: "elm";
         action: STATE_SET "visible" 0.0;
         target: "elm.text";
      }
      program { name: "icon_hide";
         signal: "elm,state,icon,hidden";
         source: "elm";
         action: STATE_SET "default" 0.00;
         target: "elm.swallow.icon";
      }
      program { name: "icon_show";
         signal: "elm,state,icon,visible";
         source: "elm";
         action: STATE_SET "visible" 0.00;
         target: "elm.swallow.icon";
      }
      program { name: "disabled";
         signal: "elm,state,disabled";
         source: "elm";
         action: STATE_SET "disabled" 0.00;
         target: "event";
         target: "clipper";
      }
      program { name: "enabled";
         signal: "elm,state,enabled";
         source: "elm";
         action: STATE_SET "default" 0.00;
         target: "clipper";
         target: "event";
      }
      program { name: "close_focused";
         signal: "mouse,in";
         source: "icon_close";
         action: STATE_SET "focused" 0.0;
         target: "icon_close";
      }
      program { name: "close_unfocused";
         signal: "mouse,out";
         source: "icon_close";
         action: STATE_SET "default" 0.0;
         target: "icon_close";
      }
      program { name: "close_clicked";
         signal: "mouse,down,1";
         source: "icon_close";
         action: STATE_SET "clicked" 0.0;
         target: "icon_close";
      }
      program { name: "close_unclicked";
         signal: "mouse,up,1";
         source: "icon_close";
         action: STATE_SET "default" 0.0;
         target: "icon_close";
         after: "close_emit";
      }
      program { name: "close_emit";
         action: SIGNAL_EMIT "tab,close" "eflete";
      }
   }
}


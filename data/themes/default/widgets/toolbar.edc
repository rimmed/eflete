               to_x: "clipper_arrow_right";
               to_y: "elm.swallow.content";
            }
            rel2 {
               to_x: "clipper_arrow_right";
               to_y: "elm.swallow.content";
            }
            image {
               normal: "icon_arrow_right.png";
            }
         }
         description { state: "clicked" 0.0;
            max: 16 16;
            color_class: "select";
            rel1 {
               to_x: "clipper_arrow_right";
               to_y: "elm.swallow.content";
            }
            rel2 {
               to_x: "clipper_arrow_right";
               to_y: "elm.swallow.content";
            }
            image {
               normal: "icon_arrow_right.png";
            }
         }
      }

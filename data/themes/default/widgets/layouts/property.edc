group { name: "elm/layout/property/color";
   images {
      image: "color-background-50x19.png" COMP;
      image: "color-disable-50x19.png" COMP;
      image: "color-shadow.png" COMP;
   }
   parts {
      part { name: "base";
         type: SPACER;
         description { state: "default" 0.0;
            min: 50 19;
            max: 50 19;
         }
      }
      part { name: "bg";
         type: IMAGE;
         description { state: "default" 0.0;
            rel1 {
               to: "base";
            }
            rel2 {
               to: "base";
            }
            image {
               normal: "color-background-50x19.png";
               middle: DEFAULT;
            }
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         description { state: "default" 0.0;
            rel1 {
               to: "base";
            }
            rel2 {
               to: "base";
            }
         }
      }
      part { name: "disabler";
         type: IMAGE;
         description { state: "default" 0.0;
            visible: 0;
            rel1 {
               to: "bg";
            }
            rel2 {
               to: "bg";
            }
            image {
               normal: "color-disable-50x19.png";
               middle: DEFAULT;
            }
         }
         description { state: "disabled" 0.0;
            rel1 {
               to: "bg";
            }
            rel2 {
               to: "bg";
            }
            image {
               normal: "color-disable-50x19.png";
               middle: DEFAULT;
            }
         }
      }
      part { name: "shadow";
         type: IMAGE;
         repeat_events: 1;
         description { state: "default" 0.0;
            rel1 {
               to: "base";
            }
            rel2 {
               to: "base";
            }
            image {
               normal: "color-shadow.png";
               border: 7 7 6 7;
               middle: DEFAULT;
            }
         }
      }
      part { name: "event";
         type: RECT;
         description { state: "default" 0.0;
            color: 0 0 0 0;
            rel1 {
               to: "base";
            }
            rel2 {
               to: "base";
            }
         }
         description { state: "disabled" 0.0;
            visible: 0;
         }
      }

   }
   programs {
      program { name: "enabled";
         signal: "elm,state,enabled";
         source: "elm";
         action: STATE_SET "default" 0.00;
         target: "disabler";
         target: "event";
      }
      program { name: "disabled";
         signal: "elm,state,disabled";
         source: "elm";
         action: STATE_SET "disabled" 0.00;
         target: "disabler";
         target: "event";
      }
      program { name: "clicked";
         signal: "mouse,clicked,1";
         source: "event";
         action: SIGNAL_EMIT "clicked" "eflete";
      }
   }
}

#define GENLIST_DIALOGUE_INHERIT_BODY( _PARTS ) \
      data.item: "treesize" 0; \
      parts { \
         GENLIST_PART_BG_NORMAL_OFF \
         GENLIST_PART_BOTTOM_LINE_OFF \
         GENLIST_PART_BG_DIALOGUE \

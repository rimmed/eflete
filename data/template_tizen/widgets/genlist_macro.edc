               set_state(PART:"focus_part_dialogue", "bottom_focused", 0.0); \
            else if (!strcmp(st, "default") || !strcmp(st, "selected") || !strcmp(st, "2depth")) \
               set_state(PART:"focus_part_dialogue", "focused", 0.0); \
         } \
      } \

					color: 0 0 0 0;
				}
			}
			part { name: "bg_left";
				mouse_events: 0;
				scale: 1;
				description { state: "default" 0.0;
					fixed: 1 1;
					min: 142 MAGNIFIER_HEIGHT_FIXED;
					align: 0.0 0.0;
					rel1 { relative: 0.0 0.0; to: "bg"; }

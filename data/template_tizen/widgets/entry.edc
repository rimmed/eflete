						normal: "copy&paste_Magnifier_middle.png";
						border: 1 38 20 44;
					}
					image.middle: 1;
					fill.smooth: 0;
				}
			}
			part { name: "bg_right";
				mouse_events: 0;
				scale: 1;
				description { state: "default" 0.0;
					fixed: 1 1;
					min: 142 MAGNIFIER_HEIGHT_FIXED;
					rel1 { relative: 1.0 0.0; to: "bg_middle_right"; }
					rel2.to: "bg";
					image {
						normal: "copy&paste_Magnifier_right.png";
						border: 1 21 20 44;
					}
					image.middle: 1;
					fill.smooth: 0;
				}
			}
			part { name: "swallow";
				type: SWALLOW;
				mouse_events: 0;
				scale: 1;
